{
  "name": "ben-rowan/doctrine-assert",
  "description": "A collection of PHPUnit database assertions based on Doctrine2",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Rowan"
    }
  ],
  "require": {
    "php": "^8.1",
    "phpunit/phpunit": "^10.0",
    "doctrine/orm": "^2.6",
    "symfony/finder": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "BenRowan\\DoctrineAssert\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BenRowan\\DoctrineAssert\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "@stable",
    "phpmd/phpmd" : "@stable",
    "mikey179/vfsstream": "@stable",
    "symfony/yaml": "@stable",
    "fakerphp/faker": "@stable",
    "symfony/cache": "^6.0"
  }
}
